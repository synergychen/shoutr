<h4>Viewing <%= @user.email %>'s profile</h4>

<%= render "follow_button", user: @user %>

<p><%= @user.email %> is following</p>

<%= render @followed_users %>

<p><%= @user.email %>'s favorite shouts </p>

<ul>
  <% @favorite_shouts.each do |shout| %>
    <li><%= render shout.subject %></li>
  <% end %>
</ul>


